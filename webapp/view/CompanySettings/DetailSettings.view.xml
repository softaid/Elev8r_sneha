<mvc:View
	controllerName="sap.ui.elev8rerp.componentcontainer.controller.CompanySettings.CompanySettings"
	xmlns="sap.uxap"
	xmlns:f="sap.ui.layout.form"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	>
	<m:Page
		title="{i18n>companySettingsTitle}"
		showHeader="{device>/system/phone}"
		showNavButton="{device>/system/phone}"
		navButtonPress="onNavButtonPress">
		<ObjectPageLayout
			id="objectPageLayout">
			<headerTitle>
				<ObjectPageHeader					
					objectSubtitle="{i18n>systemSettingsDescriptionTitle}">
					<actions>
						<ObjectPageHeaderActionButton
							id="save"
							text="{i18n>save}"
							hideText="false"
							press="onSavePressed"
							type="Emphasized"/>
						<ObjectPageHeaderActionButton
							id="cancel"
							text="{i18n>cancel}"
							press="onCancelPressed"
							hideText="false"/>
					</actions>
				</ObjectPageHeader>
			</headerTitle>
			<headerContent>
				<m:MessageStrip
					text="{
						parts: [
							'i18n>systemSettingsLastLoginTitle',
							'view>/currentUser',
							'view>/lastLogin'
						],
						formatter: 'jQuery.sap.formatMessage'
						}"
					type="Information"
					showIcon="true">
				</m:MessageStrip>
			</headerContent>
			<sections>
				<ObjectPageSection>
				<subSections>
					<ObjectPageSubSection
						id="shopOwnerId"
						title="{i18n>shopTabTitle}">
						<blocks>
							<ObjectPageLazyLoader
								id="shopOwnerLazyLoader">
								<f:SimpleForm
									title="{i18n>editSystemConfigurationTitle}"
									columnsL="2"
									columnsM="2"
									editable="true"
									layout="ResponsiveGridLayout">
									<f:content>									
								</f:content>
								</f:SimpleForm>
							</ObjectPageLazyLoader>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
				</ObjectPageSection>
				
				<ObjectPageSection>
					<subSections>
						<ObjectPageSubSection
							id="shopConfigurationId"
							title="{i18n>shopConfigurationTitle}">
							<blocks>
								<ObjectPageLazyLoader
									id="shopConfigurationLazyLoader">
									<f:SimpleForm
										title="{i18n>editSystemConfigurationTitle}"
										columnsL="2"
										columnsM="2"
										editable="true"
										layout="ResponsiveGridLayout" >
										<f:content>
										<core:Title text=""/>
											<m:Label
												text="{i18n>companyName}"
												labelFor="companyNameInput"/>
											<m:Input
												id="companyName"
												type="Text"
												width="100%"
												value="{view>/companyname}"/>
											<m:Label
												labelFor="addressInput"
												text="{i18n>companyAddress}"/>		
											<m:TextArea										
											    rows="2"		
												id=""
												type="Text"
												width="100%"
												value="{view>/companyaddress}"
												laberFor="addressInput"/>
											<!-- <m:Label
												labelFor="addressTownInput"
												text="{i18n>ownerBlock}"/>
											<m:Input
												id="addressTownInput"
												type="Text"
												width="100%"
												value="12345 Newport"/>			-->																			
											<m:Label
												labelFor="addressInternateAddressInput"
												text="{i18n>ownerInternateAddress}"/>
											<m:Input
												id="addressInternateAddressInput"
												type="Text"
												width="100%"
												value="{view>/companyemail}"/>
											<m:Label
												labelFor="addressTelephoneNumberInput"
												text="{i18n>ownerTelephoneNumber}"/>
											<m:Input
												id="addressTelephoneNumber"
												type="Text"
												width="100%"
												value="{view>/companycontact}"/>											
										<core:Title text="" />
											<!-- <m:Label
												labelFor="phoneNumberInput"
												text="{i18n>ownerStreetNo}"/>
											<m:Input
												id="phoneNumberInput"
												type="Tel"
												width="100%"
												value="Banjara Hill"/> -->
											<!-- <m:Label
												labelFor="faxNumberInput"
												text="{i18n>ownerBuildingNo}"/>
											<m:Input
												id="faxNumberInput"
												type="Tel"
												width="100%"
												value="Shop No.205"/> -->
											<m:Label
												labelFor="zipcodeInput"
												text="{i18n>zip}"/>
											<m:Input
												id="emailInput"
												type="Email"
												width="100%"
												value="{view>/pincode}"/>
												<m:Label
												labelFor="addressStateInput"
												text="{i18n>ownerCity}"/>
											<m:Input
												id="addressStateInput"
												type="Text"
												width="100%"
												value="{view>/city}"/>
											<m:Label
												labelFor="addressFaxInput"
												text="{i18n>ownerFax}"/>
											<m:Input
												id="addressFax"
												type="Text"
												width="100%"
												value="{view>/faxnumber}"/>		
											<m:Label
												labelFor="addresAdditionalPhoneInput"
												text="{i18n>ownerAdditionalPhone}"/>
											<m:Input
												id="addressAdditionalPhone"
												type="Text"
												width="100%"
												value="{view>/contactno2}"/>										
									</f:content>
									</f:SimpleForm>
								</ObjectPageLazyLoader>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>

				<ObjectPageSection>
					<subSections>
						<ObjectPageSubSection
							title="{i18n>shopAccountingTitle}">
							<blocks>
								<ObjectPageLazyLoader>
									<f:SimpleForm
										title="{i18n>editSystemConfigurationTitle}"
										columnsL="2"
										columnsM="2"
										editable="true"
										layout="ResponsiveGridLayout" >
										<f:content>	
										<core:Title text="" />										
												<m:Label
													labelFor="panNumberInput"
													text="{i18n>panNumber}"/>
												<m:Input
													type="Text"
													width="100%"
													value="YBJG190456"/>
												<m:Label
													labelFor="panCircle"
													text="{i18n>panCircle}"/>
												<m:Input
													type="Text"
													width="100%"
													value="Maharashtra"/>
												<m:Label
													labelFor="panWard"
													text="{i18n>panWard}"/>
												<m:Input
													type="Text"
													width="100%"
													value="Pune MH"/>
												<m:Label
													labelFor="panOfficer"
													text="{i18n>panOfficer}"/>
												<m:Input
													type="Text"
													width="100%"
													value="Pune MH"/>												
												<m:Label
													labelFor="lstVat"
													text="{i18n>lstVat}"/>
												<m:Input
													type="Text"
													width="100%"
													value="VAT2390"/>
												<m:Label
													labelFor="GSTInput"
													text="{i18n>storeGST}"/>
												<m:Input
													type="Text"
													width="100%"
													value="GST5489"/>
											<core:Title text="" />
												<m:Label
													labelFor="exemptionInput"
													text="{i18n>storeExemption}"/>
												<m:Input
													type="Url"
													width="100%"
													value="20%"/>
												<m:Label
													labelFor="storeTanInput"
													text="{i18n>storeTan}"/>
												<m:Input
													type="TAN"
													width="100%"
													value="PBYK2390876"/>
												<m:Label
													labelFor="storeTaxInput"
													text="{i18n>storeTax}"/>
												<m:Input
													type="Tax"
													width="100%"
													value="TD88990067"/>												
												 <m:Label
													labelFor="storeServiceTypeInput"
													text="{i18n>storeServiceType}"/>
												<m:Select  id="selectid" forceSelection="false">
														<core:Item key="1" text="All"/>
														<core:Item key="2" text="Adidas" />
														<core:Item key="3" text="Rebok" />
												</m:Select> 													
												<m:Label
													labelFor="accessTypeInput"
													text="{i18n>assessType}"/>
												<m:Input
													type="input"
													width="100%"
													value="Regional"/>
												<m:Label
													labelFor="companyTypeInput"
													text="{i18n>companyType}"/>
												<m:Input
													type="input"
													width="100%"
													value="Regional"/>
												<m:Label
													labelFor="businessTypeInput"
													text="{i18n>businessType}"/>
												<m:Input
													type="input"
													width="100%"
													value="Regional"/>
												<m:Label
													labelFor="tinInput"
													text="{i18n>businessTin}"/>
												<m:Input
													type="input"
													width="100%"
													value="Regional"/>
										</f:content>
									</f:SimpleForm>
								</ObjectPageLazyLoader>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
			</sections>
		</ObjectPageLayout>
	</m:Page>
</mvc:View>
