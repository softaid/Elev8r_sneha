<core:View displayBlock="true" controllerName="sap.ui.elev8rerp.componentcontainer.controller.Common.DashboardCBFSetting" height="100%"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form" 
    xmlns:core="sap.ui.core">
	                <VBox fitContainer="true">
                                    <form:Form editable="true">
                                        <form:toolbar>			
			                            </form:toolbar>
                                        <form:layout>
                                            <form:ResponsiveGridLayout
                                                labelSpanXL="4"
                                                labelSpanL="6"
                                                labelSpanM="4"
                                                labelSpanS="12"
                                                adjustLabelSpan="false"
                                                emptySpanXL="0"
                                                emptySpanL="0"
                                                emptySpanM="0"
                                                emptySpanS="0"
                                                columnsXL="3"
                                                columnsL="2"
                                                columnsM="2"
                                                singleContainerFullSize="false" />
                                        </form:layout>
                                        <form:formContainers>
                                        <form:FormContainer title="">
					                            <form:formElements>                                                    
                                            <form:FormElement label="{i18n>cbfSettingLabelDefaultChicksItem}">
                                                <form:fields>
                                                    <Button class="sapUiSmallMarginEnd" icon="{cbfModel>/defaultchickitemid}" 
                                                    type="{cbfModel>/defaultchickitemidtype}" width="10%"/>
                                                </form:fields>
                                            </form:FormElement>
                                            <form:FormElement label="{i18n>cbfSettingLabelDOCItem}">
                                                <form:fields>
