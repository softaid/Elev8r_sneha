<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="{i18n>noDataFound}"
		title="{i18n>HatcherySettingDialogSelectHatcherySettingParameter}"	
		contentHeight="300px"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/hatcherysettings'
		}">
		<ColumnListItem>
			<cells>
				<Text title="" text="{servicecharges}" />
                <Text text="{settingdaysper}" />
                <Text text="{hatchingdaysper}" />
                <Text text="{biproductreq}" />
                <Text text="{warehouselocation}" />
                <Text text="{overheadcostperegg}" />
                <Text text="{overheadexpensegl}" />
                
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>HatcherySettingDialogServiceCharges}" />
				</header>
			</Column>
			<!-- <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="Gap In Setting" />
				</header>
			</Column> -->
            <!-- <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="Fertility(%)" />
				</header>
			</Column> -->
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogSettingDaysPeriod}" />
				</header>
			</Column>
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogHatcheryDaysPeriod}" />
				</header>
			</Column>
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogBiProductRequired}" />
				</header>
			</Column>
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogWarehouseLocation}" />
				</header>
			</Column>
            <!-- <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="Candling Days Peroid" />
				</header>
			</Column> -->
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogOverheadCostPerEgg}" />
				</header>
			</Column>
            <Column minScreenWidth="Tablet" demandPopin="true" >
				<header>
					<Text text="{i18n>HatcherySettingDialogOverheadExpenseGL}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>