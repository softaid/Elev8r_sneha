<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data Found"
		title="Select Item"	
		contentHeight="300px"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/feedmillqualitycheck',
			sorter : {
				path : 'no',
				descending : false
			}
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="" text="{docEntry}" />
				<Text text="{ackNumber}" />
                <Text text="{itemCode}" />
                <Text text="{itemName}" />
                <Text text="{date}" />
				<Text text="{vendorName}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="DocEntry" />
				</header>
			</Column>
            <Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End" >
				<header>
					<Text text="Acknowledgement Number" />
				</header>
			</Column>
            <Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End" >
				<header>
					<Text text="Item Code" />
				</header>
			</Column>
             <Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End" >
				<header>
					<Text text="Item Name" />
				</header>
			</Column>
             <Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End" >
				<header>
					<Text text="Date" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End" >
				<header>
					<Text text="Vendor Name" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>