<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data Found"
		title="Breeder Perofrmance"		
		contentHeight="300px"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/breederperformance',
			sorter : {
				path : 'code',
				descending : false
			}
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="" text="{code}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="Code" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>