<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data Found"
		title="Select Medicine Schedule"
		contentHeight="300px"
		search="handleMedicineSearch"
		confirm="handleMScheduleClose"
		cancel="handleMScheduleClose"
		items="{
			path : 'medicinescheduleModel>/modelData',
			sorter : {
				path : 'code',
				descending : false
			}
		}">
		<ColumnListItem>
			<cells>
				<Text text="{medicinescheduleModel>id}" />
				<Text text="{medicinescheduleModel>schedulename}" />
                <Text text="{medicinescheduleModel>fromdate}" />
                <Text text="{medicinescheduleModel>todate}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column demandPopin="true" >
				<header>
					<Text text="Doc No." />
				</header>
			</Column>
			<Column demandPopin="true" >
				<header>
					<Text text="Name" />
				</header>
			</Column>
            <Column
				demandPopin="true" >
				<header>
					<Text text="From Date" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="To Date" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>